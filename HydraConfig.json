{
	"skeletonType": "CohortCharacterizationStudy",
	"skeletonVersion": "v0.0.1",
	"requiredHydraVersion": "v0.0.1",
	"actions":[{
		"type": "fileNameFindAndReplace",
		"input": "packageName",
		"find": "SkeletonCohortCharacterization"
	},{
		"type": "stringFindAndReplace",
		"input": "packageName",
		"find": "SkeletonCohortCharacterization",
		"exclude": "**/*.jar"
	},
	{
    "type": "jsonArrayToCsv",
    "input": "cohorts",
    "mapping": [{"source": "id", "target": "cohortId"},
                {"source": "id", "target": "atlasId"},
                {"source": "name", "target": "name", "modifiers": ["convertToFileName"]}],
    "output": "inst/settings/CohortsToCreate.csv"
  },{
    "type": "jsonArrayToJson",
    "input": "cohorts",
    "fileName": "name",
    "payload": "expression",
    "output": "inst/cohorts"
  },{
    "type": "jsonArrayToSql",
    "input": "cohorts",
    "fileName": "name",
    "payload": "expression",
    "output": "inst/sql/sql_server"
  },
	
	{
		"type": "jsonToJson",
		"input": "",
		"output": "inst/settings/StudySpecification.json"
	}]
}